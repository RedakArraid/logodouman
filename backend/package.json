{
  "name": "logodouman-backend",
  "version": "2.0.0",
  "description": "Backend API pour LogoDouman - E-commerce complet",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "migrate": "node scripts/migrate.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "fix:prisma": "node fix-prisma.js",
    "setup": "node fix-prisma.js && npm run migrate",
    "reset": "prisma db push --force-reset && npm run migrate",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "zod": "^3.22.4",
    "@prisma/client": "^5.7.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  },
  "keywords": [
    "e-commerce",
    "api",
    "nodejs",
    "express",
    "prisma",
    "postgresql"
  ],
  "author": "LogoDouman Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
